    <formulas>
        <name>CloseDate</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS(TODAY(), 2)</expression>
    </formulas>
    <formulas>
        <description>Formula to create Opportunity Name from Insured Full Name and Policy Number captured in screen flow</description>
        <name>Opportunity_Name</name>
        <dataType>String</dataType>
        <expression>{!Insured_Full_Name} + &quot; - &quot; + IF(ISBLANK({!Policy_Number}), TEXT(YEAR(TODAY())) + TEXT(MONTH(TODAY())) + TEXT(DAY(TODAY())), {!Policy_Number})</expression>
    </formulas>
    <formulas>
        <name>OpportunityNameFormula</name>
        <dataType>String</dataType>
        <expression>{!Insured_Full_Name} + &quot; - &quot; + {!Policy_Number}</expression>
    </formulas>
    <formulas>
        <name>PolicynumberGenerated</name>
        <dataType>String</dataType>
        <expression>{!Insured_Full_Name} + &quot; - &quot; + TEXT(YEAR(TODAY())) + LPAD(TEXT(MONTH(TODAY())), 2, &quot;0&quot;) + LPAD(TEXT(DAY(TODAY())), 2, &quot;0&quot;)</expression>
    </formulas>
