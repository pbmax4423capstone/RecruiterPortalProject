<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>63.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Did_We_Find_a_Contact</name>
        <label>Did We Find a Contact?</label>
        <locationX>182</locationX>
        <locationY>971</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Contact_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ActualSenderContact</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Case_Contact</targetReference>
            </connector>
            <label>Contact Found</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_This_a_Service_User</name>
        <label>Is This a Service User?</label>
        <locationX>380</locationX>
        <locationY>755</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ServiceUserMatchVar</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Email_Extracted</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Email_Extracted</name>
        <label>Is Email Extracted?</label>
        <locationX>182</locationX>
        <locationY>815</locationY>
        <defaultConnectorLabel>No Email Found</defaultConnectorLabel>
        <rules>
            <name>Email_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExtractedSenderEmail</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Email is Blank</label>
        </rules>
        <rules>
            <name>Valid_Email</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExtractedSenderEmail</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Actual_Sender_Contact</targetReference>
            </connector>
            <label>Valid Email</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>ExtractedSenderEmail</name>
        <dataType>String</dataType>
        <expression>IF(
  AND(
    CONTAINS($Record.Description, &quot;From:&quot;),
    CONTAINS(
      MID(
        $Record.Description,
        FIND(&quot;From:&quot;, $Record.Description),
        FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
      ),
      &quot;&lt;&quot;
    ),
    CONTAINS(
      MID(
        $Record.Description,
        FIND(&quot;From:&quot;, $Record.Description),
        FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
      ),
      &quot;&gt;&quot;
    )
  ),
  MID(
    MID(
      $Record.Description,
      FIND(&quot;From:&quot;, $Record.Description),
      FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
    ),
    FIND(&quot;&lt;&quot;, 
      MID(
        $Record.Description,
        FIND(&quot;From:&quot;, $Record.Description),
        FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
      )
    ) + 1,
    FIND(&quot;&gt;&quot;, 
      MID(
        $Record.Description,
        FIND(&quot;From:&quot;, $Record.Description),
        FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
      )
    ) - 
    FIND(&quot;&lt;&quot;, 
      MID(
        $Record.Description,
        FIND(&quot;From:&quot;, $Record.Description),
        FIND(&quot;\n&quot;, $Record.Description &amp; &quot;\n&quot;, FIND(&quot;From:&quot;, $Record.Description)) - FIND(&quot;From:&quot;, $Record.Description)
      )
    ) - 1
  ),
  &quot;&quot;
)</expression>
    </formulas>
    <interviewLabel>Update Contact on forwarded email case {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Update Contact on forwarded email case</label>
    <processMetadataValues>
        <name>BuilderType</name>
