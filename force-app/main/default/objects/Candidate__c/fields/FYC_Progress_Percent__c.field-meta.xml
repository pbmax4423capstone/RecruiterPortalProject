<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FYC_Progress_Percent__c</fullName>
    <description>Percentage of FYC requirement met (toward $2,500).</description>
    <formula>IF(
    Total_FYC__c &gt; 0,
    MIN(Total_FYC__c / 2500, 1),
    0
)</formula>
    <inlineHelpText>Progress toward $2,500 FYC requirement (capped at 100%).</inlineHelpText>
    <label>FYC Progress</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
