System.debug('Total Ci-First Interviews: ' + interviews.size());
System.debug('--- By Interviewer ---');
for (String interviewer : interviewerCounts.keySet()) {
    System.debug(interviewer + ': ' + interviewerCounts.get(interviewer));
}
