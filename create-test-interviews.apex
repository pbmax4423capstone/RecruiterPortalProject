System.debug('Total interviews to create: ' + interviewData.size());

// Create Contacts first
List<Contact> newContacts = new List<Contact>();
for (Map<String, String> data : interviewData) {
    String firstName = data.get('FirstName');
    String lastName = data.get('LastName');
    Contact c = new Contact(
        FirstName = firstName,
        LastName = lastName,
        Email = firstName.toLowerCase().replaceAll('[^a-z]', '') + '.' + lastName.toLowerCase().replaceAll('[^a-z]', '') + '@example.com'
    );
    newContacts.add(c);
}

insert newContacts;
System.debug('Created ' + newContacts.size() + ' contacts');

// Create Candidates linked to Contacts
List<Candidate__c> newCandidates = new List<Candidate__c>();
for (Integer i = 0; i < newContacts.size(); i++) {
    Contact c = newContacts[i];
    Map<String, String> data = interviewData[i];
    
    Candidate__c cand = new Candidate__c(
        Contact__c = c.Id,
        First_Name__c = data.get('FirstName'),
        Last_Name__c = data.get('LastName'),
        Recruiting_Source__c = 'Referral',
        Status__c = 'Active/In Process'
    );
    newCandidates.add(cand);
}

insert newCandidates;
System.debug('Created ' + newCandidates.size() + ' candidates');

// Now create the Interview records
List<Interview__c> interviews = new List<Interview__c>();
for (Integer i = 0; i < newCandidates.size(); i++) {
    Candidate__c cand = newCandidates[i];
    Map<String, String> data = interviewData[i];
    
    Interview__c interview = new Interview__c(
        Candidate__c = cand.Id,
        Interview_Type__c = 'Ci-First',
        Interview_Status__c = 'Completed',
        Date_Completed__c = Date.valueOf(data.get('DateCompleted'))
    );
    
    // Set interviewer (multi-select picklist)
    String interviewer = data.get('Interviewer');
    if (String.isNotBlank(interviewer)) {
        interview.Interviewer_s__c = interviewer;
    }
    
    interviews.add(interview);
}

insert interviews;
System.debug('Created ' + interviews.size() + ' interviews');

// Output summary by interviewer
Map<String, Integer> interviewerCounts = new Map<String, Integer>();
for (Map<String, String> data : interviewData) {
    String interviewer = data.get('Interviewer');
    if (!interviewerCounts.containsKey(interviewer)) {
        interviewerCounts.put(interviewer, 0);
    }
    interviewerCounts.put(interviewer, interviewerCounts.get(interviewer) + 1);
}

System.debug('=== TEST DATA CREATION COMPLETE ===');
